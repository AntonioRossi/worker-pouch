{
  "name": "socket-pouch",
  "version": "0.1.0",
  "description": "PouchDB over websockets (WORK IN PROGRESS)",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/nolanlawson/socket-pouch.git"
  },
  "keywords": [
    "pouch",
    "pouchdb",
    "plugin",
    "seed",
    "couch",
    "couchdb"
  ],
  "author": "",
  "license": "Apache",
  "bugs": {
    "url": "https://github.com/nolanlawson/socket-pouch/issues"
  },
  "scripts": {
    "test-node": "TEST_DB=testdb,http://localhost:5984/testdb istanbul test ./node_modules/mocha/bin/_mocha test/test.js",
    "test-browser": "./bin/test-browser.js",
    "jshint": "jshint -c .jshintrc *.js test/test.js",
    "test": "npm run jshint && ./bin/run-test.sh",
    "build": "mkdirp dist npm run browserify && npm run min",
    "browserify": "browserify index.js | ./bin/es3ify.js | derequire > dist/pouchdb.socket.js",
    "min": "uglifyjs dist/pouchdb.socket.js -mc > dist/pouchdb.socket.min.js",
    "dev": "browserify test/test.js > test/test-bundle.js && npm run dev-server",
    "dev-server": "./bin/dev-server.js",
    "coverage": "npm test --coverage && istanbul check-coverage --lines 100 --function 100 --statements 100 --branches 100"
  },
  "dependencies": {
    "argsarray": "0.0.1",
    "blob-util": "^1.1.1",
    "bluebird": "^2.9.24",
    "debug": "^2.1.3",
    "engine.io": "^1.5.1",
    "engine.io-client": "^1.5.1",
    "eval": "^0.1.0",
    "inherits": "^2.0.1",
    "lie": "^2.6.0",
    "pouchdb": "pouchdb/pouchdb#3741-3740",
    "pouchdb-binary-util": "^1.0.0",
    "pouchdb-extend": "^0.1.2"
  },
  "devDependencies": {
    "browserify": "^9.0.8",
    "chai": "^1.9.1",
    "corsproxy": "^0.2.14",
    "derequire": "^2.0.0",
    "es3ify": "^0.1.3",
    "es5-shim": "^4.1.1",
    "http-server": "~0.5.5",
    "istanbul": "^0.2.7",
    "jshint": "^2.3.0",
    "memwatch": "^0.2.2",
    "mkdirp": "^0.5.0",
    "mocha": "^1.18.2",
    "phantomjs": "^1.9.7-5",
    "pouchdb-http-proxy": "^0.10.4",
    "request": "^2.36.0",
    "sauce-connect-launcher": "^0.4.2",
    "selenium-standalone": "^3.0.2",
    "uglify-js": "^2.4.13",
    "watchify": "^3.1.0",
    "wd": "^0.2.21"
  },
  "browser": {
    "./lib/shared/buffer.js": "./lib/shared/buffer-browser.js"
  }
}
